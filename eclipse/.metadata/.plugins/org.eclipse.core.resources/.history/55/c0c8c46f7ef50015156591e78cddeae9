	public void preInitialization(FMLPreInitializationEvent event) {
		
	}
	
	@Mod.EventHandler
	public void initialization(FMLInitializationEvent event) {
		
	}
	
	@Mod.EventHandler
	public void postInitialization(FMLPostInitializationEvent event) {
		
	}
}
